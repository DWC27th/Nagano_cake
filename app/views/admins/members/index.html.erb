
<div class = "col-xs-9">
<h1>会員一覧</h1>


<table class="table table-striped">
<thead>
　<tr>
　  <th></th>
　　<th>会員id</th>
　　<th>氏名</th>
　　<th>メールアドレス</th>
　　<th>ステータス</th>
　　<th colspan="4"></th>
　　<th></th>
　</tr>
</thead>

 <tbody>
  <% @members.each do |member| %>
   <tr>
    <td><%= member.id %></td>
    <td><%= member.first_name %></td>
    <td><%= member.email %></td>
    <td><%= member.member_status %></td>
   </tr>
  <% end %>
 </tbody>
　
</table>

</div>
